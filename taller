#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	
	while(T--)
	{
	    int X,Y;
	    cin>>X>>Y;
	    
	    if(X>Y)
	    {
	        cout<<"A"<<endl;
	    }
	    else if(Y>X)
	    {
	        cout<<"B"<<endl;
	    }
	    else{
	        cout<<"same"<<endl;
	    }
	}
	return 0;
}
